@model InstagramMVC.ViewModels.NotaterViewModel

<div class="container">
    <h2>Mine Notater</h2>
    
    <!-- Liste over mine notater -->
    <div class="row mt-5">
        @foreach (var note in Model.Notes)
        {
            <div class="col-md-4">
                @await Html.PartialAsync("_NoteList", note)
            </div>
        }
    </div>
    <!-- Knapp for Ã¥ navigere til Create-siden for notater -->
    <div class="mt-4 text-center">
        <a asp-action="Create" asp-controller="Notat" class="btn btn-primary">Opprett nytt notat</a>
    </div>
</div>