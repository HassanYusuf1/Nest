@model InstagramMVC.ViewModels.NotaterViewModel

<!DOCTYPE html>
<html>
<head>
    <title>Alle Notater</title>
    <link rel="stylesheet" href="~/css/layout.css" />
</head>
<body class="notes">
    <div class="container">
        <h1>Notater</h1>
        <div class="row flex-column align-items-center">
            @foreach (var note in Model.Notes)
            {
                <div class="col-12 mb-4 notes-card">
                    @{
                        ViewData["returnUrl"] = Url.Action("Notater");
                    }
                    @await Html.PartialAsync("_NoteList", note, ViewData)
                </div>
            }
        </div>
    </div>
</body>
</html>
